{
    "version": 2,
    "builds": [
      {
        "src": "api/openapi.cjs",
        "use": "@vercel/node"
      },
      {
        "src": "src/*",
        "use": "@vercel/static-build",
        "config": { "distDir": "build" }
      }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/api/vercel.cjs" },
      { "src": "/(.*)", "dest": "/src/index.html" }
    ]
  }